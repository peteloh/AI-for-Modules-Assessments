    "#01702868\n",
    "import math as mt\n",
    "def Summation(N):\n",
    "    if N == 1:\n",
    "        # exit condition\n",
    "        f = 1\n",
    "    else:\n",
    "        # recursive formula\n",
    "        f = (N**8)/mt.factorial(N-1) + Summation(N-1)\n",
    "    return f\n",
    "\n",
    "a = Summation(15)\n",
    "print(a)"
   ]
