    "#01720430\n",
    "#TaskD\n",
    "import math as mt\n",
    "\n",
    "CID=[0,1,7,0,1,2,6,3] #CID\n",
    "a=CID[6-1] \n",
    "\n",
    "def Sum(n):\n",
    "    n-1!=0\n",
    "    if n==1:\n",
    "        S=1\n",
    "    else:\n",
    "        S=n**a/(mt.factorial(n-1))+Sum(n-1) #Apply recursion\n",
    "    return S\n",
    "n=15\n",
    "S=Sum(n)\n",
    "print(S)\n"
   ]
